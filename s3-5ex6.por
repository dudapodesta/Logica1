programa {
  funcao inicio() {
    cadeia nome
    real nota1,nota2,media
    escreva("Digite seu nome: ")
    leia(nome)
    escreva("Entre com sua primeira nota: ")
    leia(nota1)
    escreva("Entre com sua segunda nota: ")
    leia(nota2)
    media=(nota1+nota2)/2
    escreva(nome, " sua média final foi ", media)
    
  }
}
